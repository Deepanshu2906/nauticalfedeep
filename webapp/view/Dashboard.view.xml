<mvc:View xmlns:tnt="sap.tnt"
xmlns:form="sap.ui.layout.form"
xmlns:l="sap.ui.layout"
controllerName="nauticalfe.controller.Dashboard"
xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
xmlns:core="sap.ui.core"
>
    
   <Page id="_IDGenPage1"  >
   <headerContent >
            <!-- Your header content goes here -->
            <Bar class="customHeader" >
                <contentLeft>
                    <Button
                        class="customHeaderButton"
                        icon="sap-icon://nav-back"
                        press="onBackPress"
                    />
                    <Image
                        src="../images/Ingenx.png"
                        height="2rem"
                        width="6rem"
                    />
                    <Text
                        class="textBid"
                        text="Nautical Dashboard"
                    />
                </contentLeft>
                <contentRight >
                    <core:Icon
                        src="sap-icon://search"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                    <core:Icon
                        src="sap-icon://person-placeholder"
                        class="size1"
                        color="#FFFFFF"
                    >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                    
                </contentRight>
            </Bar>
        </headerContent>
      <VBox id="_IDGenVBox1" class="vPanel">
         <Panel id="_IDGenPanel1" class="panel" width="650px" height="650px">
            
            <ObjectListItem id="_IDGenObjectListItem1" intro="Select Transaction Type" ></ObjectListItem>

            <Button id="_IDGenButton1" text="Transaction Data" press="TransData" class="Trans" width="30%" icon="sap-icon://request"></Button>
         
            <Button id="_IDGenButton2" text="Master Data" press="MasterData" class="Mastr" width="30%" icon="sap-icon://visits"></Button>
         </Panel>   
            
         <Panel id="_IDGenPanel2"  class="panel1">
            <Image id="_IDGenImage1" src="../images/nautical.png" width="400px" height="400px"   ></Image>
         </Panel>
      </VBox>
   </Page>
   
    

</mvc:View> 